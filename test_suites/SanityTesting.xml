<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Sanity test">
	<listeners>
		<listener class-name="automation_suite.BaseClass.TestListeners"></listener>
	</listeners>


	<test name="Luma Login">
		<classes>
			<class name="automationSuite.TestCases.LoginUser" />
		</classes>
	</test>

	<test name="Log off test">
		<classes>
			<class name="automationSuite.TestCases.LogoffUser"></class>
		</classes>
	</test>

	<test name="Negative Validation">
		<classes>
			<class name="automationSuite.TestCases.NegativeValidations"></class>
		</classes>
	</test>
</suite>